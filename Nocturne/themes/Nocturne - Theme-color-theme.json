				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "HTML Tag",
			"scope": [
			  "entity.name.tag.html"
			],
			"settings": {
			  "foreground": "#0096ff" 
			}
		  },
		  {
			"name": "HTML Attribute",
			"scope": [
			  "entity.other.attribute-name.html"
