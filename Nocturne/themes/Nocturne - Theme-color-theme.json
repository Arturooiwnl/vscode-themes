{
	"name": "Nocturne - Theme",
	"colors": {
		"editor.background": "#0d0d0d",
		"editor.foreground": "#cccccc",
		"activityBar.background": "#0d0d0d",
		"activityBarBadge.background": "#1e1e1e",
		"sideBar.background": "#121212",
		"sideBar.foreground": "#cccccc",
		"sideBarTitle.foreground": "#ffffff",
		"statusBar.background": "#0d0d0d",
		"statusBar.foreground": "#888888",
		"titleBar.activeBackground": "#0d0d0d",
		"titleBar.activeForeground": "#ffffff",
		"editorLineNumber.foreground": "#3a3a3a",
		"editorCursor.foreground": "#ffffff",
		"editor.selectionBackground": "#333366aa",
		"editor.inactiveSelectionBackground": "#22224488",
		"editorIndentGuide.background1": "#222222",
		"editorIndentGuide.activeBackground1": "#ffea29",
		"editor.lineHighlightBackground": "#ffffff00"
	  },
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder",
				"variable.sup",
	
			],
			"settings": {
				"foreground": "#97efff"
			}
		},
		{
			"name": "constantes",
			"scope": [
				"constant"
			],
			"settings": {
				"foreground": "#73a6f3"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
